{
    "model_type": "decoder_only",
    "architecture": "transformer",
    "model_name": "Shaili-Phi3-Mini-4bit",
    "base_model": "Phi-3-mini",
    "purpose": "inference",
    
    "model_dimensions": {
        "hidden_size": 768,
        "num_attention_heads": 8,
        "num_hidden_layers": 16,
        "intermediate_size": 3072,
        "max_position_embeddings": 131072
    },
    
    "quantization": {
        "method": "4-bit",
        "type": "nf4",
        "compute_dtype": "bfloat16",
        "use_double_quant": true,
        "compression_level": "high",
        "memory_optimization": "inference_optimized"
    },
    
    "generation_config": {
        "temperature": [0.7, 0.9],
        "top_p": [0.9, 0.95],
        "repetition_penalty": [1.1, 1.2],
        "max_new_tokens": 768,
        "early_stopping": true,
        "num_beams": 4,
        "no_repeat_ngram_size": 2
    },
    
    "optimization_strategies": {
        "context_cache": {
            "sliding_window": true,
            "window_size": 8000,
            "summary_frequency": 8000
        },
        "token_efficiency": {
            "domain_tokens": ["<med>", "<math>", "<prog>", "<soc>", "<sport>"],
            "special_tokens": ["<inst>", "<resp>", "<sys>"]
        }
    },
    
    "special_tokens": {
        "pad_token": "<pad>",
        "eos_token": "<eos>",
        "bos_token": "<bos>",
        "unk_token": "<unk>",
        "sep_token": "<sep>",
        "inst_token": "<inst>",
        "resp_token": "<resp>",
        "sys_token": "<sys>"
    },
    
    "domain_tokens": [
        "<med>", "<math>", "<prog>", "<soc>", "<sport>"
    ]
}
